.tribe-common {
	/* -------------------------------------------------------------------------
	 *
	 * CTA
	 *
	 * Example:
	 * <a class="tribe-common-cta">Link Text</a>
	 * <a class="tribe-common-cta tribe-common-cta--alt">Link Text Alt</a>
	 *
	 * ------------------------------------------------------------------------- */

	.tribe-common-cta {
		@mixin desktop-body-3-bold;
		@mixin anchor-default;
	}

	.tribe-common-cta--alt {
		@mixin anchor-alt;
	}
}
